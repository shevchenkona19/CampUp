{
  "name": "campup-model",
  "version": "0.0.1",
  "description": "",
  "main": "src/server/server.js",
  "scripts": {
    "start": "docker-compose up db web",
    "clean": "rimraf build/",
    "start:client": "npm run clean; webpack",
    "start:server": "nodemon",
    "dc:install": "docker-compose run npm install && npm run dc:migrate",
    "migration:create": "./node_modules/.bin/sequelize migration:create --name",
    "dc:migrate": "docker-compose up -d db && docker-compose run web node_modules/.bin/wait-port db:5432 && npm run migration:up && docker-compose stop db",
    "migration:up": "docker-compose run web node_modules/.bin/sequelize db:migrate --config ./development.json",
    "dc:uninstall": "docker-compose down -v",
    "dc:start": "concurrently --kill-others \"npm run start:client\" \"npm run start:server\""
  },
  "author": "Nikita Shevchenko",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "babel-preset-react-hmre": "^1.1.1",
    "body-parser": "latest",
    "concurrently": "^3.6.1",
    "express": "latest",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "nodemon": "^1.18.3",
    "path": "^0.12.7",
    "node-sass": "^4.9.2",
    "pg": "^7.4.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "resolve-url-loader": "^2.3.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.38.0",
    "source-map-support": "^0.5.4",
    "sass-loader": "^7.0.1",
    "sequelize-cli": "^4.0.0",
    "style-loader": "^0.21.0",
    "wait-port": "^0.2.2",
    "webpack": "^4.10.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "presets": [
        "react-hmre"
      ]
    }
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "webpack-cli": "^3.1.0"
  }
}
